    "betterScripts": {
        "start": {
            "command": "npm-run-all clean client:build:dev server:build:dev && npm-run-all --parallel client:build:dev:watch server:build:dev:watch server:run",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "build:stable": {
            "command": "npm-run-all clean client:build:stable server:build:stable",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "build:testing": {
            "command": "npm-run-all clean client:build:testing server:build:testing",
            "env": {
                "NODE_ENV": "testing"
            }
        },
        "server:run": {
            "command": "nodemon --watch dist/server --exec \"node dist/server/server.js\"",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "server:build:dev": {
            "command": "webpack --config webpack.server.config.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "server:build:dev:watch": {
            "command": "webpack --config webpack.server.config.js --watch",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "server:build:stable": {
            "command": "webpack --config webpack.server.config.js",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "server:build:testing": {
            "command": "webpack --config webpack.server.config.js",
            "env": {
                "NODE_ENV": "testing"
            }
        },
        "client:build:stable": {
            "command": "cross-env IS_SERVER=n webpack --config webpack.client.config.js",
            "env": {
                "NODE_ENV": "production"
            }
        },
        "client:build:testing": {
            "command": "cross-env IS_SERVER=n webpack --config webpack.client.config.js",
            "env": {
                "NODE_ENV": "testing"
            }
        },
        "client:build:dev": {
            "command": "cross-env IS_SERVER=n webpack --config webpack.client.config.js",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        },
        "client:build:dev:watch": {
            "command": "cross-env IS_SERVER=n webpack --config webpack.client.config.js --watch",
            "env": {
                "NODE_ENV": "development",
                "DEBUG": "app:*"
            }
        }
    }
